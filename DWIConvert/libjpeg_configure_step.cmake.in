set(ENV{CC} "@CMAKE_C_COMPILER@ @CMAKE_C_COMPILER_ARG1@")
set(ENV{CFLAGS} "@CMAKE_C_FLAGS@ @CMAKE_C_FLAGS_RELEASE@")
set(ENV{CXX} "@CMAKE_CXX_COMPILER@ @CMAKE_CXX_COMPILER_ARG1@")
set(ENV{CXXFLAGS} "@CMAKE_CXX_FLAGS@ @CMAKE_CXX_FLAGS_RELEASE@")
set(ENV{LDFLAGS} "@CMAKE_LINKER_FLAGS@ @CMAKE_LINKER_FLAGS_RELEASE@")

set(libjpeg_prefix @CMAKE_CURRENT_BINARY_DIR@/libjpeg-prefix/src)
execute_process(COMMAND sh ${libjpeg_prefix}/libjpeg/configure --prefix=@PREREQS@ --enable-static=yes --enable-shared=no
  WORKING_DIRECTORY ${libjpeg_prefix}/libjpeg-build
  RESULT_VARIABLE config_result
  OUTPUT_VARIABLE config_output
  ERROR_VARIABLE config_error)

message("Result=${config_result}")
message("Outpute=${config_output}")
message("Error=${config_error}")
